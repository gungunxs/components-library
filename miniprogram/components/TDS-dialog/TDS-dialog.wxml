<!--components/DYWH-dialog/DYWH-dialog.wxml-->
<view class="mask" wx:if="{{showOut}}" bindtap="close" style="z-index: 99999;animation-name: {{maskAnimationName}};" catchtouchmove="true" data-theme="{{theme}}">
  <view class="dialog flex dialog--{{layout}} {{gradient?'Thematic-gradient':''}} {{gradient?'theme':'duoduo-card'}}-shadow" catchtap="catch" style="animation-name: {{animationName}};">
    <!-- 标题区-->
    <view class="dialog-title">
      <!-- 一级标题-->
      <text class="H3 dialog-title-head fill-availavle" wx:if="{{isHead}}">{{headTitle}}</text>
      <!-- 二级标题-->
      <text class="Paragraphs1 dialog-title-sub fill-available" wx:if="{{isSub&&(subTitle!='')}}">{{subTitle}}</text>
    </view>
    <!-- slot-->
    <slot name="swap"></slot>
    <!-- button group components-->
    <slot name="button"></slot>
  </view>
</view>